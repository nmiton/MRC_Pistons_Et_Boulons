<form  class="" method="post" action="{{ path('content_delete', {'id': content.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce contenu?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ content.id) }}">
    <button class="float-left">Supprimer</button>
</form>
